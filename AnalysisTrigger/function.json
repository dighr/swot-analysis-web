{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "msg",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "analyze",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "annpayload",
      "path": "queue-ann/{blobName}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "eopayload",
      "path": "queue-eo/{blobName}",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
